<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <bean class="in.sp.beans.Student" id="stdId1">
       <property name="name" value="abhijit"/>
        <property name="rollNo" value="100"/>
         <property name="email" value="abhijit12@gmail.com"/>
    </bean>
    
    <bean class="in.sp.beans.Student" id="stdId2">
       <property name="name" value="ram"/>
        <property name="rollNo" value="101"/>
         <property name="email" value="ram2@gmail.com"/>
    </bean>

</beans>